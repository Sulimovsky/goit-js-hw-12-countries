{{#each this}}
<div class="centered"><h2 class="title countries">{{name}}</h2></div>
<div class="flex-container">
    <div class="box-flex-container">
        <p class="capital">Capital: {{capital}}</p>
        <p class="population">Population: {{population}}</p>
        <p class="lang">Languages:</p>
        <ul class="list-lang">
            {{#each languages}}
            <li class="item"><p>{{name}}</p></li>
            {{/each}}
        </ul>
    </div>
          
        <div class="box-flex-container">
            <img src="{{flag}}" width="380" height="250" alt="{{name}}">
        </div>
</div>
{{/each}}